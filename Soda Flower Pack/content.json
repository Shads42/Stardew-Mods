{
    "Format": "2.0.0",
    "DynamicTokens": [
        {
            "Name": "SVEId",
            "Value": "FlashShifter.StardewValleyExpandedCP"
        },
        {
            "Name": "WAGId",
            "Value": "Wildflour.AtelierGoods"
        },
        {
            "Name": "ASMId",
            "Value": "Hadi.JASoda"
        }
    ],
    "Changes": [
        {
            "LogName": "Load spritesheet",
            "Action": "Load",
            "Target": "{{ModId}}/objects",
            "FromFile": "assets/{{TargetWithoutPath}}.png"
        },
        {
            "LogName": "Load default data",
            "Action": "Include",
            "FromFile": "data/objects.json, data/strings.json, machines/carbonator.json, machines/sodamaker.json, machines/syrupmaker.json, machines/creamsodamaker.json, data/gifttastes.json"
        },
        {
            "LogName": "Load Cornucipia Flowers default data",
            "Action": "Include",
            "FromFile": "machines_essential/syrupmaker.json, machines_essential/sodamaker.json, machines_essential/creamsodamaker.json, data/gifts_cornucopia.json",
            "When": {
                "HasMod | contains=Cornucopia.MoreFlowers": true
            }
        },
        {
            "LogName": "Load Cornucopia Flowers extended data",
            "Action": "Include",
            "FromFile": "machines_extended/syrupmaker.json, machines_extended/sodamaker.json, machines_extended/creamsodamaker.json, data/gifts_cornucopia_extended.json",
            "When": {
                "HasMod | contains=Cornucopia.MoreFlowers": true,
                "HasReadLetter | contains=Cornucopia.MoreFlowers_ExtendedFlowersPackEnabled": true
            }
        },
        {
            "LogName": "Load Clover items",
            "Action": "Include",
            "FromFile": "machines/sve_machines.json, data/gifts_sve.json",
            "When": {
                "HasMod | contains=FlashShifter.StardewValleyExpandedCP": true
            }
        },
        {
            "LogName": "Load default products",
            "Action": "Include",
            "FromFile": "machines/defaultproducts.json"
        } 
    ]
}